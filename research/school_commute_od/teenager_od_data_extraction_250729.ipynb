  {
   "cell_type": "raw",
   "id": "b6adab12-fa55-44e4-97a7-cb0839be32f2",
   "metadata": {},
   "source": [
    "# 1. 연단위 총 컬럼과 일 평균\n",
    "\n",
    "query = '''\n",
    "-- 1. 방학과 주말 제거\n",
    "WITH school_days AS\n",
    "(\n",
    "    SELECT *\n",
    "    FROM teenager.tb_linked_transport_teenager_2024_transfer_applied\n",
    "    WHERE 운행일자 NOT LIKE '202401%' \n",
    "        AND 운행일자 NOT LIKE '202402%' \n",
    "        AND 운행일자 NOT LIKE '202407%' \n",
    "        AND 운행일자 NOT LIKE '202408%'\n",
    "    -- 평일만 남기기\n",
    "        AND STRFTIME(STRPTIME(승차일시, '%Y%m%d%H%M%S'), '%w') NOT IN ('0', '6') \n",
    "        AND STRPTIME(SUBSTR(승차일시, 1, 8), '%Y%m%d') NOT IN (SELECT holiday_date FROM temp_holidays_2024)\n",
    "),\n",
    "dbscan_transfer_applied AS \n",
    "(\n",
    "    SELECT dbs.*,\n",
    "        tr.new_pk\n",
    "    FROM teenager.tb_cardid_dbscan_clustered dbs\n",
    "    LEFT JOIN tb_station_transfer_modified tr\n",
    "        ON dbs.정류장ID = tr.정류장ID\n",
    "        AND dbs.지역코드 = tr.지역코드\n",
    "        AND dbs.교통수단구분 = tr.교통수단구분\n",
    "),\n",
    "cluster_mapped AS(\n",
    "    SELECT od.*,\n",
    "        dbs_h1.클러스터구분 AS 승차_주거지클러스터구분,\n",
    "        dbs_h2.클러스터구분 AS 하차_주거지클러스터구분,\n",
    "        dbs_s1.클러스터구분 AS 승차_학교클러스터구분,\n",
    "        dbs_s2.클러스터구분 AS 하차_학교클러스터구분\n",
    "    FROM school_days od\n",
    "    -- 승차&주거지 정류장 유무\n",
    "    LEFT JOIN dbscan_transfer_applied dbs_h1\n",
    "        ON od.가상카드번호 = dbs_h1.가상카드번호\n",
    "        AND od.승차정류장PK = dbs_h1.new_pk\n",
    "        AND dbs_h1.클러스터구분 IN ('home', 'school')\n",
    "    -- 하차&주거지 정류장 유무\n",
    "    LEFT JOIN dbscan_transfer_applied dbs_h2\n",
    "        ON od.가상카드번호 = dbs_h2.가상카드번호\n",
    "        AND od.하차정류장PK = dbs_h2.new_pk\n",
    "        AND dbs_h2.클러스터구분 = 'home'\n",
    "    -- 승차&학교 정류장 유무\n",
    "    LEFT JOIN dbscan_transfer_applied dbs_s1\n",
    "        ON od.가상카드번호 = dbs_s1.가상카드번호\n",
    "        AND od.승차정류장PK = dbs_s1.new_pk\n",
    "        AND dbs_s1.클러스터구분 = 'school'\n",
    "    -- 하차&학교 정류장 유무\n",
    "    LEFT JOIN dbscan_transfer_applied dbs_s2\n",
    "        ON od.가상카드번호 = dbs_s2.가상카드번호\n",
    "        AND od.하차정류장PK = dbs_s2.new_pk\n",
    "        AND dbs_s2.클러스터구분 = 'school'\n",
    "        -- 등교 패턴\n",
    "    WHERE (dbs_h1.클러스터구분 = 'home' AND dbs_s2.클러스터구분 = 'school')\n",
    "        -- 하교 패턴\n",
    "        OR (dbs_h2.클러스터구분 = 'home' AND dbs_s1.클러스터구분 = 'school')\n",
    "),\n",
    "case_applied AS(\n",
    "    SELECT *,\n",
    "            CASE WHEN 승차_주거지클러스터구분 = 'home' AND 하차_학교클러스터구분 = 'school' THEN 'morning'\n",
    "            WHEN 하차_주거지클러스터구분 = 'home' AND 승차_학교클러스터구분 = 'school' THEN 'evening' \n",
    "            END AS trip_type\n",
    "    FROM cluster_mapped\n",
    ")\n",
    "INSERT INTO teenager.tb_school_od_2024\n",
    "SELECT \n",
    "    MAX(승차정류장ID) AS origin_station_id,\n",
    "    MAX(하차정류장ID) AS destination_station_id,\n",
    "    승차정류장PK AS origin_station_pk,\n",
    "    하차정류장PK AS destination_station_pk,\n",
    "    MAX(승차정류장명칭) AS origin_station_name,\n",
    "    MAX(하차정류장명칭) AS destination_station_name,\n",
    "    MAX(승차지역코드) AS origin_area_code,\n",
    "    MAX(하차지역코드) AS destination_area_code,\n",
    "    MAX(승차교통수단구분) AS origin_station_type,\n",
    "    MAX(하차교통수단구분) AS destination_station_type,\n",
    "    trip_type,\n",
    "    COUNT(*) AS annual_count,\n",
    "    AVG(총탑승시간) AS average_time,\n",
    "    MEDIAN(총탑승시간) AS median_time,\n",
    "    AVG(총이용거리) AS average_distance,\n",
    "    MEDIAN(총이용거리) AS median_distance,\n",
    "    MAX(승차_x) AS 승차_x,\n",
    "    MAX(승차_y) AS 승차_y,\n",
    "    MAX(하차_x) AS 하차_x,\n",
    "    MAX(하차_y) AS 하차_y\n",
    "FROM case_applied\n",
    "GROUP BY 승차정류장PK, 하차정류장PK, trip_type\n",
    "'''\n",
    "\n",
    "con.execute(query).df()"
   ]
  },
