   "source": [
    "# 새 정제 테이블 생성\n",
    "query = '''\n",
    "DROP TABLE IF EXISTS teenager.tb_linked_transport_teenager_2024_transfer_applied;\n",
    "CREATE TABLE teenager.tb_linked_transport_teenager_2024_transfer_applied\n",
    "(   운행일자 VARCHAR,\n",
    "    가상카드번호 VARCHAR,\n",
    "    트랜잭션ID INT,\n",
    "    승차정류장ID INT,\n",
    "    하차정류장ID INT,\n",
    "    승차정류장PK VARCHAR,\n",
    "    하차정류장PK VARCHAR,\n",
    "    승차정류장명칭 VARCHAR,\n",
    "    하차정류장명칭 VARCHAR,\n",
    "    승차지역코드 VARCHAR,\n",
    "    하차지역코드 VARCHAR,\n",
    "    승차일시 VARCHAR,\n",
    "    하차일시 VARCHAR,\n",
    "    승차교통수단구분 VARCHAR(1),\n",
    "    하차교통수단구분 VARCHAR(1),\n",
    "    총이용거리 NUMERIC,\n",
    "    총탑승시간 NUMERIC,\n",
    "    승차_x DOUBLE PRECISION,\n",
    "    승차_y DOUBLE PRECISION,\n",
    "    하차_x DOUBLE PRECISION,\n",
    "    하차_y DOUBLE PRECISION)\n",
    "'''\n",
    "con.execute(query).df()"
   ]
  },
  {
   "cell_type": "raw",
   "id": "a121d2f9-8c34-4eec-89e5-22111c9c5ccc",
   "metadata": {},
   "source": [
    "# 지하철 환승역인 경우 -> 정류장ID_min, x_5179_centroid, y_5179_centroid 존재\n",
    "# 결측치: 결합무결성 1.7% 탈락\n",
    "# 테이블에 매핑해보고 \n",
    "query = '''\n",
    "    WITH station AS(\n",
    "        SELECT *\n",
    "        FROM tb_station_transfer_modified\n",
    "    )\n",
    "    INSERT INTO teenager.tb_linked_transport_teenager_2024_transfer_applied\n",
    "    SELECT\n",
    "        lt.운행일자,\n",
    "        lt.가상카드번호,\n",
    "        lt.트랜잭션ID,\n",
    "        COALESCE(pk1.정류장ID_min, lt.승차정류장ID) AS 승차정류장ID,\n",
    "        COALESCE(pk2.정류장ID_min, lt.하차정류장ID) AS 하차정류장ID,\n",
    "        pk1.new_pk AS 승차정류장PK,\n",
    "        pk2.new_pk AS 하차정류장PK,\n",
    "        pk1.정류장명칭 AS 승차정류장명칭,\n",
    "        pk2.정류장명칭 AS 하차정류장명칭,\n",
    "        lt.승차지역코드,\n",
    "        lt.하차지역코드,\n",
    "        lt.승차일시,\n",
    "        lt.하차일시,\n",
    "        lt.승차교통수단구분,\n",
    "        lt.하차교통수단구분,\n",
    "        lt.총이용거리,\n",
    "        lt.총탑승시간,\n",
    "        COALESCE(pk1.x_5179_centroid, pk1.x_5179) AS 승차_x,\n",
    "        COALESCE(pk1.y_5179_centroid, pk1.y_5179) AS 승차_y,\n",
    "        COALESCE(pk2.x_5179_centroid, pk2.x_5179) AS 하차_x,\n",
    "        COALESCE(pk2.y_5179_centroid, pk2.y_5179) AS 하차_y\n",
    "    FROM teenager.tb_linked_transport_teenager_2024 lt\n",
    "    -- 승차 통행\n",
    "    LEFT JOIN station pk1\n",
    "        ON lt.승차정류장ID::BIGINT = pk1.정류장ID::BIGINT\n",
    "        AND lt.승차지역코드 = pk1.지역코드\n",
    "        AND lt.승차교통수단구분 = pk1.교통수단구분\n",
    "    -- 하차 통행\n",
    "    LEFT JOIN station pk2\n",
    "        ON lt.하차정류장ID::BIGINT = pk2.정류장ID::BIGINT\n",
    "        AND lt.하차지역코드 = pk2.지역코드\n",
    "        AND lt.하차교통수단구분 = pk2.교통수단구분\n",
    "    WHERE pk1.new_pk IS NOT NULL \n",
    "        AND pk2.new_pk IS NOT NULL\n",
    "    '''\n",
    "con.execute(query).df() # 365일 구분 없이 있음"
   ]
  },
