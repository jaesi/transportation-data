  {
   "cell_type": "raw",
   "id": "ea5057c0-2950-4b80-995e-3bab2d840b69",
   "metadata": {
    "scrolled": true
   },
   "source": [
    "# db에 청소년만 로드하기\n",
    "# 1. 달과 해당하는 날짜 세트로 만들기\n",
    "from datetime import datetime, timedelta \n",
    "\n",
    "start_date = datetime(2024, 1, 1)\n",
    "end_date = datetime(2024, 12, 31)\n",
    "\n",
    "# 달별로 순회하면서 목적통행으로 묶고 인서트 스택 하는 방식\n",
    "for i in range((end_date-start_date).days +1):\n",
    "    date = start_date + timedelta(days=i)\n",
    "    yyyymm = date.strftime('%Y%m')\n",
    "    yyyymmdd = date.strftime('%Y%m%d')\n",
    "    # print(f'processing %% {yyyymm}-{yyyymmdd}')\n",
    "    q = f'''\n",
    "    INSERT INTO teenager.tb_linked_transport_teenager_2024\n",
    "    WITH original AS (\n",
    "        SELECT 운행일자,\n",
    "            가상카드번호,\n",
    "            정산지역코드,\n",
    "            카드구분코드,\n",
    "            정산사차량ID,\n",
    "            교통수단코드,\n",
    "            정산사노선ID,\n",
    "            승차일시,\n",
    "            정산사승차정류장ID,\n",
    "            하차일시,\n",
    "            정산사하차정류장ID,\n",
    "            트랜잭션ID,\n",
    "            환승건수,\n",
    "            \"이용자유형코드(시스템)\",\n",
    "            이용자수,\n",
    "            이용거리,\n",
    "            탑승시간\n",
    "        FROM read_csv(\"/home/kdata/import_data/TB_KTS_DWTCD_METROPOLITAN/{yyyymm}/TB_KTS_DWTCD_METROPOLITAN_{yyyymmdd}.csv\", types= {{'운행일자':'VARCHAR',\n",
    "            '가상카드번호':'VARCHAR',\n",
    "            '카드구분코드':'VARCHAR',\n",
    "            '정산사차량ID':'VARCHAR',\n",
    "            '교통수단코드':'VARCHAR',\n",
    "            '정산사노선ID':'VARCHAR',\n",
    "            '승차일시':'VARCHAR',\n",
    "            '정산사승차정류장ID':'VARCHAR',\n",
    "            '하차일시':'VARCHAR',\n",
    "            '정산사하차정류장ID':'VARCHAR',\n",
    "            '트랜잭션ID':'INT',\n",
    "            '환승건수':'VARCHAR',\n",
    "            '이용자유형코드(시스템)':'VARCHAR',\n",
    "            '이용자수':'INT',\n",
    "            '이용거리':'NUMERIC',\n",
    "            '탑승시간':'NUMERIC'}})\n",
    "        WHERE \"이용자유형코드(시스템)\" = '03'\n",
    "        AND 하차일시 IS NOT NULL\n",
    "    ),\n",
    "    -- 목적통행으로 집계\n",
    "    base_set AS(\n",
    "        SELECT 운행일자,\n",
    "            가상카드번호,\n",
    "            트랜잭션ID,\n",
    "            SUM(이용거리) AS 총이용거리,\n",
    "            SUM(탑승시간) AS 총탑승시간,\n",
    "            MAX(환승건수) AS 최대환승건수\n",
    "        FROM original og\n",
    "        GROUP BY 운행일자, 가상카드번호, 트랜잭션ID\n",
    "    )\n",
    "    SELECT b.운행일자,\n",
    "        b.가상카드번호,\n",
    "        b.트랜잭션ID,\n",
    "        og1.정산사승차정류장ID AS 승차정류장ID,\n",
    "        og2.정산사하차정류장ID AS 하차정류장ID,\n",
    "        og1.승차일시 AS 승차일시,\n",
    "        og2.하차일시 AS 하차일시,\n",
    "        og1.정산지역코드 AS 승차지역코드,\n",
    "        og2.정산지역코드 AS 하차지역코드,\n",
    "        og1.교통수단코드 AS 승차교통수단코드,\n",
    "        og2.교통수단코드 AS 하차교통수단코드,\n",
    "        b.최대환승건수,\n",
    "        b.총이용거리,\n",
    "        b.총탑승시간\n",
    "    FROM base_set b\n",
    "    JOIN original og1\n",
    "        ON b.운행일자 = og1.운행일자\n",
    "        AND b.가상카드번호 = og1.가상카드번호\n",
    "        AND b.트랜잭션ID = og1.트랜잭션ID\n",
    "        AND og1.환승건수 = 0\n",
    "    JOIN original og2\n",
    "        ON b.운행일자 = og2.운행일자\n",
    "        AND b.가상카드번호 = og2.가상카드번호\n",
    "        AND b.트랜잭션ID = og2.트랜잭션ID\n",
    "        AND b.최대환승건수 = og2.환승건수\n",
    "    '''\n",
    "    # con.execute(q)"
   ]
  },
