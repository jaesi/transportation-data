    "# table 생성\n",
    "query = '''\n",
    "DROP TABLE IF EXISTS teenager.tb_linked_transport_teenager_2024;\n",
    "CREATE TABLE teenager.tb_linked_transport_teenager_2024\n",
    "(운행일자 VARCHAR,\n",
    "가상카드번호 VARCHAR,\n",
    "트랜잭션ID INT,\n",
    "승차정류장ID VARCHAR,\n",
    "하차정류장ID VARCHAR,\n",
    "승차일시 VARCHAR,\n",
    "하차일시 VARCHAR,\n",
    "승차지역코드 VARCHAR,\n",
    "하차지역코드 VARCHAR,\n",
    "승차교통수단코드 VARCHAR,\n",
    "하차교통수단코드 VARCHAR,\n",
    "최대환승건수 INT,\n",
    "총이용거리 NUMERIC,\n",
    "총탑승시간 NUMERIC);\n",
    "'''\n",
    "# con.execute(query).df()\n",
    "# con.execute('select * from teenager.tb_linked_transport_teenager_2024;').df()"
   ]
