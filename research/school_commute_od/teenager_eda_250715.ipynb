   "source": [
    "# 일반적 방학기간: \n",
    "# 여름: 7월 20일 ~ 8월 20일 \n",
    "# 겨울: 12월 20일 ~ 1월 20일\n",
    "\n",
    "# 데이터 상: 7,8월 / 1,2월 등하교 시간대 빈도가 적음\n",
    "# 4개월 제거\n",
    "# 0) 총 행수: 247553159\n",
    "# 1) 방학 제거 시: 173712643: 70.2%\n",
    "# 2) 휴일 제거 시: 130090013: 52.6%\n",
    "# 3) 통행횟수 1회 초과인 건들: 129688797: 52.3%\n",
    "\n",
    "query = '''\n",
    "WITH only_school AS(\n",
    "    SELECT *\n",
    "    FROM teenager.tb_linked_transport_teenager_2024\n",
    "    WHERE 운행일자 NOT LIKE '202401%' \n",
    "        AND 운행일자 NOT LIKE '202402%' \n",
    "        AND 운행일자 NOT LIKE '202407%' \n",
    "        AND 운행일자 NOT LIKE '202408%'\n",
    "    -- 평일만 남기기\n",
    "        AND STRFTIME(STRPTIME(승차일시, '%Y%m%d%H%M%S'), '%w') NOT IN ('0', '6') \n",
    "        AND STRPTIME(SUBSTR(승차일시, 1, 8), '%Y%m%d') NOT IN (SELECT holiday_date FROM temp_holidays_2024)\n",
    "),\n",
    "-- 통행횟수 집계\n",
    "annual_counts AS(\n",
    "    SELECT \n",
    "        가상카드번호\n",
    "    FROM only_school \n",
    "    GROUP BY 가상카드번호\n",
    "    HAVING COUNT(*) > 1\n",
    "),\n",
    "-- 랜덤샘플 1000명\n",
    "random_samples AS(\n",
    "    SELECT\n",
    "        가상카드번호\n",
    "    FROM annual_counts\n",
    "    USING SAMPLE 1000\n",
    ")\n",
    "SELECT *\n",
    "FROM only_school os\n",
    "JOIN random_samples rs\n",
    "    ON os.가상카드번호 = rs.가상카드번호\n",
    ";\n",
    "'''\n",
    "\n",
    "con.execute(query).df()"
   ]
  }
 ],
