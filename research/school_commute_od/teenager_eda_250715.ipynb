   "source": [
    "# 가장 많이 승차하는 정류장 TOP 10 & 하차 정류장 TOP 10 - 평일 기준 \n",
    "query = '''\n",
    "WITH date_dist AS(\n",
    "    SELECT *,\n",
    "        CASE \n",
    "        WHEN ISODOW(strptime(하차일시, '%Y%m%d%H%M%S'))<6 THEN '평일'\n",
    "        ELSE '주말'\n",
    "        END AS '일자'\n",
    "    FROM teenager.tb_linked_transport_teenager_2024\n",
    "    WHERE 일자 = '평일'\n",
    "        AND 하차교통수단구분 = 'B'\n",
    "), \n",
    "top10 AS(\n",
    "    SELECT 하차정류장ID, \n",
    "        하차지역코드, \n",
    "        하차교통수단구분,\n",
    "        COUNT(*) AS count\n",
    "    FROM date_dist\n",
    "    GROUP BY 하차정류장ID, 하차지역코드, 하차교통수단구분\n",
    "    ORDER BY COUNT(*) DESC\n",
    "    LIMIT 10\n",
    ")\n",
    "SELECT t.*,\n",
    "    st.정류장명칭\n",
    "FROM top10 t\n",
    "JOIN read_csv('import_data/TB_KTS_STTN/202412/TB_KTS_STTN_20241231.csv') st\n",
    "    ON t.하차정류장ID = st.정류장ID\n",
    "    AND t.하차지역코드::VARCHAR = st.지역코드\n",
    "    AND t.하차교통수단구분 = st.교통수단구분\n",
    "ORDER BY t.count DESC\n",
    "'''\n",
    "con.execute(query).df()"
   ]
  },
