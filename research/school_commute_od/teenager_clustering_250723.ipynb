   "source": [
    "# 이슈 해결\n",
    "# 이슈 1: 김포공항이 부천종합운동장역 옆에 잘못된 포인트 하나 있음\n",
    "print(len(station_1231))\n",
    "station_1231 = station_1231.query(\"정류장ID != 1980 or 지역코드 != '11100'\")\n",
    "print(len(station_1231))\n",
    "\n",
    "# 이슈 2: x <-> y 바뀐 사례 체인지\n",
    "crs_switched = station_1231.query(\"정류장GPSY좌표 > 100\")\n",
    "print(f\"좌표값 바껴있는 경우 {len(crs_switched)}개\")\n",
    "crs_switched['정류장GPSX좌표_2'] = crs_switched.정류장GPSX좌표\n",
    "crs_switched.정류장GPSX좌표 = crs_switched.정류장GPSY좌표\n",
    "crs_switched.정류장GPSY좌표 = crs_switched.정류장GPSX좌표_2\n",
    "crs_switched.drop(columns = '정류장GPSX좌표_2', inplace=True)\n",
    "\n",
    "# 재결합 \n",
    "station_fixed = pd.concat([station_1231.query(\"정류장GPSY좌표 < 100\"), crs_switched])\n",
    "print(len(station_fixed))\n",
    "\n",
    "# DuckDB에 등록\n",
    "# con.register(\"temp_station_fixed\", station_fixed)"
   ]
