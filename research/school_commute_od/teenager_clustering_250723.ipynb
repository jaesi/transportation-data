    "# Create into table\n",
    "query = '''\n",
    "    DROP TABLE IF EXISTS tb_station_fixed_20241231;\n",
    "    CREATE TABLE tb_station_fixed_20241231\n",
    "    (정류장ID VARCHAR,\n",
    "     지역코드 VARCHAR,\n",
    "     교통수단구분 VARCHAR(1),\n",
    "     정류장명칭 VARCHAR,\n",
    "     x DOUBLE PRECISION,\n",
    "     y DOUBLE PRECISION);\n",
    "    INSERT INTO tb_station_fixed_20241231\n",
    "    (SELECT \n",
    "        정류장ID,\n",
    "        지역코드,\n",
    "        교통수단구분,\n",
    "        정류장명칭,\n",
    "        정류장GPSX좌표 AS x,\n",
    "        정류장GPSY좌표 AS y\n",
    "    FROM temp_station_fixed);\n",
