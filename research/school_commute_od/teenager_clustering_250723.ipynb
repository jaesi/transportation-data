   "id": "967d1f46-8ba5-42a2-a3ee-d573b9fc80a9",
   "metadata": {},
   "source": [
    "# Time Window - 주거지 테이블\n",
    "\n",
    "query = '''\n",
    "WITH school_days AS(\n",
    "    SELECT *,\n",
    "        strptime(승차일시, '%Y%m%d%H%M%S') AS board_time,\n",
    "        strptime(하차일시, '%Y%m%d%H%M%S') AS alight_time\n",
    "    FROM teenager.tb_linked_transport_teenager_2024\n",
    "    WHERE 운행일자 NOT LIKE '202401%' \n",
    "        AND 운행일자 NOT LIKE '202402%' \n",
    "        AND 운행일자 NOT LIKE '202407%' \n",
    "        AND 운행일자 NOT LIKE '202408%'\n",
    "    -- 평일만 남기기\n",
    "        AND STRFTIME(STRPTIME(승차일시, '%Y%m%d%H%M%S'), '%w') NOT IN ('0', '6') \n",
    "        AND STRPTIME(SUBSTR(승차일시, 1, 8), '%Y%m%d') NOT IN (SELECT holiday_date FROM temp_holidays_2024)\n",
    "),\n",
    "residence_candidates AS (\n",
    "SELECT 가상카드번호,\n",
    "    승차정류장ID AS 정류장ID,\n",
    "    board_time AS 일시,\n",
    "    승차지역코드 AS 지역코드,\n",
    "    승차교통수단구분 AS 교통수단구분,\n",
    "    '승차' AS type\n",
    "FROM school_days s\n",
    "WHERE hour(s.board_time) BETWEEN 5 AND 10 \n",
    "\n",
    "UNION\n",
    "\n",
    "SELECT 가상카드번호,\n",
    "    하차정류장ID AS 정류장ID,\n",
    "    alight_time AS 일시,\n",
    "    하차지역코드 AS 지역코드,\n",
    "    하차교통수단구분 AS 교통수단구분,\n",
    "    '하차' AS type\n",
    "FROM school_days s\n",
    "WHERE hour(s.alight_time) BETWEEN 15 AND 22 \n",
    "),\n",
    "-- 정류장 결합\n",
    "stations AS (\n",
    "    SELECT 지역코드, \n",
    "        교통수단구분, \n",
    "        정류장ID,\n",
    "        정류장GPSY좌표, \n",
    "        정류장GPSX좌표 \n",
    "    FROM temp_station_fixed\n",
    "    WHERE 정류장GPSY좌표 IS NOT NULL \n",
    "    AND 정류장GPSX좌표 IS NOT NULL\n",
    ")\n",
    "INSERT INTO teenager.tb_residence_candidates_2024\n",
    "\n",
    "SELECT rc.*,\n",
    "    s1.정류장GPSX좌표 AS x,\n",
    "    s1.정류장GPSY좌표 AS y,\n",
    "FROM residence_candidates rc\n",
    "JOIN stations s1\n",
    "    ON rc.지역코드 = s1.지역코드\n",
    "    AND rc.교통수단구분 = s1.교통수단구분\n",
    "    AND rc.정류장ID = s1.정류장ID\n",
    "'''\n",
    "\n",
    "con.execute(query).df()"
   ]
  },
