   "source": [
    "from pyproj import Transformer\n",
    "\n",
    "transformer = Transformer.from_crs(\"EPSG:4326\", \"EPSG:5179\", always_xy=True)\n",
    "\n",
    "# Transform to UTM: 4326 -> 5179\n",
    "station_fixed['x_5179'], station_fixed['y_5179'] = transformer.transform(\n",
    "    station_fixed.정류장GPSX좌표, station_fixed.정류장GPSY좌표)\n",
    "\n",
    "subway_fixed = station_fixed.query(\"교통수단구분=='T'\")\n",
    "\n",
    "# 환승역 dbscan 수행 cluster_label 생성 => 0일 경우 환승역 묶인 것\n",
    "subway_clustered = pd.DataFrame()\n",
    "for station_name, station_df in subway_fixed.groupby('정류장명칭'): # 정류장 명칭 그룹 내에서 수행\n",
    "    station_coords = station_df[['x_5179', 'y_5179']].values\n",
    "    station_df['cluster_label']=\\\n",
    "        DBSCAN(eps=400, min_samples = 2, metric='euclidean').fit_predict(station_coords)\n",
    "    subway_clustered = pd.concat([subway_clustered, station_df])\n",
    "\n",
    "subway_clustered.query(\"정류장명칭=='시청'\") # 예시 데이터: 시청"
   ]
  },
