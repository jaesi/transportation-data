    "\n",
    "transfer_subway['정류장ID_min'] = transfer_subway.groupby(['지역코드', '교통수단구분', '정류장명칭','cluster_label'])\\\n",
    "                ['정류장ID'].transform('min')\n",
    "transfer_subway['x_5179_centroid'] = transfer_subway.groupby(['지역코드', '교통수단구분', '정류장명칭','cluster_label'])\\\n",
    "                ['x_5179'].transform('mean')\n",
    "transfer_subway['y_5179_centroid'] = transfer_subway.groupby(['지역코드', '교통수단구분', '정류장명칭','cluster_label'])\\\n",
    "                ['y_5179'].transform('mean')\n",
    "\n",
    "transfer_subway_modified = pd.concat([single_subway, transfer_subway])[['정류장ID', '지역코드', '교통수단구분', '정류장명칭',\n",
    "                                                                 'x_5179', 'y_5179', '정류장ID_min', 'x_5179_centroid', \n",
    "                                                                        'y_5179_centroid']]\n",
    "print(len(transfer_subway_modified))\n",
    "transfer_subway_modified.head()"
