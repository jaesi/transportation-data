   "source": [
    "# 매핑 테이블 생성 ( N:1 )\n",
    "transfer_subway = subway_clustered.query(\"cluster_label != -1\") # 환승역 \n",
    "single_subway = subway_clustered.query(\"cluster_label == -1\") # 일반 지하철 - 노이즈 \n",
