    "# 1. 랜덤 샘플 추출 및 통행 확인"
   ]
  },
  {
   "cell_type": "raw",
   "id": "e6a62039-fec5-4ba7-b66f-ea0d06362b0b",
   "metadata": {},
   "source": [
    "query = '''\n",
    "WITH only_school AS(\n",
    "    SELECT *\n",
    "    FROM teenager.tb_linked_transport_teenager_2024\n",
    "    WHERE 운행일자 NOT LIKE '202401%' \n",
    "        AND 운행일자 NOT LIKE '202402%' \n",
    "        AND 운행일자 NOT LIKE '202407%' \n",
    "        AND 운행일자 NOT LIKE '202408%'\n",
    "    -- 평일만 남기기\n",
    "        AND STRFTIME(STRPTIME(승차일시, '%Y%m%d%H%M%S'), '%w') NOT IN ('0', '6') \n",
    "        AND STRPTIME(SUBSTR(승차일시, 1, 8), '%Y%m%d') NOT IN (SELECT holiday_date FROM temp_holidays_2024)\n",
    "),\n",
    "-- 통행횟수 집계\n",
    "annual_counts AS(\n",
    "    SELECT \n",
    "        가상카드번호\n",
    "    FROM only_school \n",
    "    GROUP BY 가상카드번호\n",
    "    HAVING COUNT(*) > 1\n",
    "),\n",
    "-- 랜덤샘플 1000명\n",
    "random_samples AS (\n",
    "    SELECT\n",
    "        가상카드번호\n",
    "    FROM annual_counts\n",
    "    USING SAMPLE 1000\n",
    "),\n",
    "-- 정류장 결합\n",
    "stations AS (\n",
    "    SELECT 지역코드, \n",
    "        교통수단구분, \n",
    "        정류장ID, \n",
    "        정류장명칭, \n",
    "        법정동코드, \n",
    "        정류장GPSY좌표, \n",
    "        정류장GPSX좌표 \n",
    "    FROM read_csv('import_data/TB_KTS_STTN/202412/TB_KTS_STTN_20241231.csv')\n",
    ")\n",
    " \n",
    "SELECT os.*,\n",
    "    s1.정류장명칭 AS 승차정류장명칭,\n",
    "    s1.정류장GPSY좌표 AS 승차_y,\n",
    "    s1.정류장GPSX좌표 AS 승차_x,\n",
    "    s2.정류장명칭 AS 하차정류장명칭,\n",
    "    s2.정류장GPSY좌표 AS 하차_y,\n",
    "    s2.정류장GPSX좌표 AS 하차_x\n",
    "FROM only_school os\n",
    "JOIN random_samples rs\n",
    "    ON os.가상카드번호 = rs.가상카드번호\n",
    "JOIN stations s1\n",
    "    ON os.승차지역코드 = s1.지역코드\n",
    "    AND os.승차교통수단구분 = s1.교통수단구분\n",
    "    AND os.승차정류장ID = s1.정류장ID\n",
    "JOIN stations s2\n",
    "    ON os.하차지역코드 = s2.지역코드\n",
    "    AND os.하차교통수단구분 = s2.교통수단구분\n",
    "    AND os.하차정류장ID = s2.정류장ID\n",
    ";\n",
    "'''\n",
    "# NULL값 비율: 0.01%\n",
    "samples_1000 = con.execute(query).df()\n",
    "samples_1000.shape"
   ]
  },
