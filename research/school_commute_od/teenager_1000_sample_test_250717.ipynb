   "id": "e15e78e1-d7b0-4144-b5d3-67d0ebeaeefc",
   "metadata": {},
   "source": [
    "from pyproj import Transformer \n",
    "\n",
    "# 좌표 변환 -> 5179\n",
    "transformer = Transformer.from_crs(\"EPSG:4326\", \"EPSG:5179\", always_xy=True)\n",
    "samples_1000['승차_x'], samples_1000['승차_y'] = transformer.transform(samples_1000['승차_x'].values, samples_1000['승차_y'].values)\n",
    "samples_1000['하차_x'], samples_1000['하차_y'] = transformer.transform(samples_1000['하차_x'].values, samples_1000['하차_y'].values)\n",
    "\n",
    "# 승하차일시 datetime 형식으로 변환\n",
    "samples_1000.승차일시 = pd.to_datetime(samples_1000.승차일시, format='%Y%m%d%H%M%S')\n",
    "samples_1000.하차일시 = pd.to_datetime(samples_1000.하차일시, format='%Y%m%d%H%M%S')"
   ]
  },
  {
   "cell_type": "raw",
   "id": "6d3bb067-b182-45ce-b36e-7131e28f32da",
   "metadata": {},
   "source": [
    "pipeline(samples_1000, morning_hours=(5,10), evening_hours=(16,22),\n",
    "         eps=800, min_samples=10, metric='manhattan')"
   ]
  },
  {
   "cell_type": "raw",
   "id": "85e7558f-932e-4cf6-ab4d-84cd48be00cb",
   "metadata": {},
   "source": [
    "from itertools import product\n",
    "# Grid-Search\n",
    "eps_list= [300, 500, 800, 1000]\n",
    "min_samples_list = [10, 15, 20]\n",
    "metric_list=['manhattan']\n",
    "morning_hours_list = [(5,9), (5,10), (5,11), (5,12)]\n",
    "evening_hours_list = [(15,18), (15,20), (15,22)]\n",
    "\n",
    "param_grid = list(product(eps_list, min_samples_list, metric_list, morning_hours_list, evening_hours_list))\n",
    "len(param_grid)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "f7ea5115-e4e5-4571-87fe-11aa887ab74d",
   "metadata": {
    "scrolled": true
   },
   "source": [
    "from joblib import Parallel, delayed\n",
    "from tqdm import tqdm\n",
    "\n",
    "results = Parallel(n_jobs=-1)\\\n",
    "(delayed(pipeline)(samples_1000, eps= eps, min_samples = min_samples,\n",
    "                  morning_hours=morning_hours, evening_hours=evening_hours, metric=metric)\n",
    " for eps, min_samples, metric, morning_hours, evening_hours in tqdm(param_grid))\n",
    "results_df = pd.DataFrame(results)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "3076193a-ddca-480c-8192-7a19e94665b4",
   "metadata": {
    "scrolled": true
   },
   "source": [
    "results_df.sort_values('both_ratio', ascending=False)"
   ]
  },
