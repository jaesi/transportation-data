   "id": "e2dba56a-f3df-49b3-a24c-8754787fc6d3",
   "metadata": {},
   "source": [
    "result_home_df, result_school_df = pipeline_to_df(samples_1000, eps=800, min_samples=10, metric='manhattan', morning_hours=(5,12), evening_hours=(15,22))\n",
    "result_home_df = result_home_df.query(\"is_main_cluster ==True\").rename(columns = {'is_main_cluster':'is_home_cluster'}).drop(columns='cluster')\n",
    "result_school_df = result_school_df.query(\"is_main_cluster ==True\").rename(columns = {'is_main_cluster':'is_school_cluster'}).drop(columns='cluster')\n",
    "\n",
    "result_home_no_dupe = result_home_df.drop_duplicates(subset=['가상카드번호', 'x', 'y'])\n",
    "result_school_no_dupe = result_school_df.drop_duplicates(subset=['가상카드번호', 'x', 'y'])\n",
    "result_home_school_df = result_home_no_dupe.merge(result_school_no_dupe, how = 'outer', on=['가상카드번호', 'x', 'y'])\n",
    "result_home_school_df.fillna(False, inplace=True)"
   ]
  },
  {
