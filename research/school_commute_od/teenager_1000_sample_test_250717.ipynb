    "# 함수 특정 가상카드 번호 지정 시 승하차 지점을 다르게 표시하도록\n",
    "import plotly.graph_objects as go\n",
    "\n",
    "def to_gis_file(df, card_id):\n",
    "    filtered_df = df[df['가상카드번호'] == card_id]\n",
    "    if filtered_df.empty:\n",
    "        print(\"카드번호에 해당하는 데이터 없음.\")\n",
    "        return\n",
    "    # 승하차시간대 컬럼으로 분리\n",
    "    board_gdf = gpd.GeoDataFrame(filtered_df, geometry = gpd.points_from_xy(filtered_df['승차_x'],\n",
    "                                                                               filtered_df['승차_y']),\n",
    "                                                                              crs = 'EPSG:4326')\n",
    "    alight_gdf = gpd.GeoDataFrame(filtered_df, geometry = gpd.points_from_xy(filtered_df['하차_x'],\n",
    "                                                                               filtered_df['하차_y']),\n",
    "                                                                              crs = 'EPSG:4326')\n",
    "    # 승하차시간대 컬럼으로 분리\n",
    "    board_gdf['승차시간대'] = board_gdf['승차일시'].astype(str).str[-6:-4].astype(int)\n",
    "    alight_gdf['하차시간대'] = alight_gdf['하차일시'].astype(str).str[-6:-4].astype(int)\n",
    "    board_gdf.to_file('output/teenager/teenager승차_4326.geojson')\n",
    "    alight_gdf.to_file('output/teenager/teenager하차_4326.geojson')\n",
    "\n",
    "# test\n",
    "to_gis_file(samples_1000, 'znYVuYYgX25l96DdkRlbtcCMeJVLejoLWIXup+LAJsw=')"
   ]
