   ]
  },
  {
   "cell_type": "raw",
   "id": "7e10c272-9268-49b8-aafc-97a36bb98e5d",
   "metadata": {
    "scrolled": true
   },
   "source": [
    "# 원본 테이블에 머지\n",
    "\n",
    "board_samples_1000 = samples_1000.merge(result_home_school_df, how = 'left', \n",
    "                  left_on = ['가상카드번호', '승차_x', '승차_y'],\n",
    "                  right_on = ['가상카드번호', 'x', 'y']).rename(columns = \n",
    "                                                          {'is_home_cluster':'승차_is_home_cluster',\n",
    "                                                           'is_school_cluster':'승차_is_school_cluster'})\\\n",
    "                .drop(columns =['x', 'y'])\n",
    "\n",
    "total_samples_1000 = board_samples_1000.merge(result_home_school_df, how = 'left', \n",
