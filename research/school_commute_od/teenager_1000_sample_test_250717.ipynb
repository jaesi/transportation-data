   "source": [
    "# 비율 산정기 \n",
    "# 주거지 클러스터 있는 행수 \n",
    "is_home_cluster_rows = ((total_samples_1000['승차_is_home_cluster'] == True) | \n",
    "                        (total_samples_1000['하차_is_home_cluster'] == True)).sum()\n",
    "# 학교 클러스터 있는 행수 \n",
    "is_school_cluster_rows = ((total_samples_1000['승차_is_school_cluster'] == True) | \n",
    "                          (total_samples_1000['하차_is_school_cluster'] == True)).sum()\n",
    "\n",
    "# 주거지와 학교 클러스터가 동시에 잡힌 행수\n",
    "is_home_school_cluster_rows = (((total_samples_1000['승차_is_home_cluster'] == True) | \n",
    "                        (total_samples_1000['하차_is_home_cluster'] == True)) & \n",
    "                        ((total_samples_1000['승차_is_school_cluster'] == True) | \n",
    "                          (total_samples_1000['하차_is_school_cluster'] == True))).sum()\n",
    "print(round(is_home_cluster_rows/len(total_samples_1000)*100, 1))\n",
    "print(round(is_school_cluster_rows/len(total_samples_1000)*100, 1))\n",
    "print(round(is_home_school_cluster_rows/len(total_samples_1000)*100, 1))"
   ]
